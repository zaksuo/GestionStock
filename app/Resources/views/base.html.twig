<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bundles/boutique/css/main.css') }}" type="text/css" media="all" />
        {% endblock %}
        {% block javascripts_libs %}
            <script type="text/javascript" src="{{ asset('bundles/boutique/js/libs/jquery-1.8.2.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/boutique/js/libs/jquery-ui-1.9.0.custom/js/jquery-ui-1.9.0.custom.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/boutique/js/libs/jquery.forms.js') }}"></script>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
            {% block content %}{% endblock %}
        {% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
