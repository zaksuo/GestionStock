<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Boutique\DatabaseBundle\Entity\FactureArticle" table="facture_article">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idRemise" type="integer" column="id_remise" nullable="true"/>
    <field name="quantite" type="integer" column="quantite" nullable="false"/>
    <many-to-one field="idArticle" target-entity="Article">
      <join-columns>
        <join-column name="id_article" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idFacture" target-entity="Facture">
      <join-columns>
        <join-column name="id_facture" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idStock" target-entity="Stock">
      <join-columns>
        <join-column name="id_stock" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
